    <style name="rating_bar_1" parent="@android:style/Widget.RatingBar">
        <item name="android:progressDrawable">@drawable/rating_bar_1</item>
    </style>

    <style name="rating_bar_2" parent="@android:style/Widget.RatingBar">
        <item name="android:progressDrawable">@drawable/rating_bar_2</item>
    </style>

    <style name="rating_bar_3" parent="@android:style/Widget.RatingBar">
        <item name="android:progressDrawable">@drawable/rating_bar_3</item>
    </style>

    <style name="progress_bar_1" parent="@android:style/Widget.ProgressBar">
        <item name="android:progressDrawable">@drawable/progress_bar_1</item>
        <item name="android:indeterminateDrawable">
            @drawable/progress_indeterminate_horizontal_holo_1
        </item>
    </style>

    <style name="progress_bar_2" parent="@android:style/Widget.ProgressBar">
        <item name="android:progressDrawable">@drawable/progress_bar_2</item>
        <item name="android:indeterminateDrawable">
            @drawable/progress_indeterminate_horizontal_holo_1
        </item>
    </style>

    <style name="spinner_limited_width">
        <item name="android:layout_gravity">center_horizontal</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:layout_width">130dp</item>
        <item name="paddingStart" tools:targetApi="jelly_bean_mr1">0dp</item>
        <item name="paddingEnd" tools:targetApi="jelly_bean_mr1">0dp</item>
        <item name="android:textColor">@color/colorPrimary</item>
        <item name="backgroundTint" tools:targetApi="lollipop">@color/colorPrimary</item>
        <item name="android:textSize">13sp</item>
    </style>
    <style name="text">
        <item name="android:textColor">@color/textcolorPrimary</item>
        <item name="android:gravity">right|center_vertical</item>
        <item name="android:textColorHint">#bbffffff</item>
    </style>
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="windowNoTitle">true</item>
        <item name="android:windowBackground">@color/window_background</item>
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:navigationBarColor" tools:targetApi="lollipop">@color/colorAccent</item>
        <item name="colorControlNormal">@color/grey_line</item>
        <item name="colorControlActivated">@color/colorPrimary</item>
        <item name="colorControlHighlight">@color/grey_line</item>

    </style>

    <style name="fade_animation">
        <item name="@android:windowEnterAnimation">@anim/fade_in</item>
        <item name="@android:windowExitAnimation">@anim/fade_out</item>
    </style>

    <style name="myDialog" parent="Theme.AppCompat.Dialog">
        <item name="windowNoTitle">true</item>
    </style>

    <style name="gravity_start">
        <item name="android:gravity">right</item>
        <item name="android:layout_gravity">right</item>
        <item name="android:layout_alignParentRight">true</item>
    </style>

    <style name="gravity_left">
        <item name="android:gravity">left</item>
        <item name="android:layout_gravity">left</item>
        <item name="android:layout_alignParentLeft">true</item>
    </style>

    <style name="toolbar_title" parent="gravity_start">
        <item name="android:layout_centerVertical">true</item>
        <item name="android:textAppearance">@style/TextAppearance.AppCompat.Widget.ActionBar.Title</item>
        <item name="android:textSize">20sp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:layout_marginRight">16dp</item>
        <item name="android:layout_gravity">right</item>
    </style>

    <style name="toolbar_title_with_icon" parent="toolbar_title">
        <item name="android:layout_marginRight">72dp</item>
    </style>
    <style name="toolbar_title_with_icon_right" parent="toolbar_title">
        <item name="android:layout_marginRight">10dp</item>
    </style>
    <style name="toolbar_image_start" parent="gravity_start">
        <item name="android:layout_centerVertical">true</item>
        <item name="android:padding">@dimen/menu_icon_padding</item>
        <item name="android:layout_width">@dimen/menu_icon_outside</item>
        <item name="android:layout_height">@dimen/menu_icon_outside</item>
        <item name="android:layout_marginRight">4dp</item>
    </style>

    <style name="floating_action_button">
        <item name="android:layout_marginLeft">16dp</item>
        <item name="android:layout_marginTop">16dp</item>
        <item name="android:layout_marginRight">16dp</item>
        <item name="android:layout_marginBottom">16dp</item>
    </style>

    <style name="toolbar_image_end" parent="gravity_end">
        <item name="android:layout_centerVertical">true</item>
        <item name="android:padding">@dimen/menu_icon_padding</item>
        <item name="android:layout_width">@dimen/menu_icon_outside</item>
        <item name="android:layout_height">@dimen/menu_icon_outside</item>
        <item name="android:layout_marginLeft">4dp</item>
    </style>
    <style name="toolbar_end" parent="gravity_end">
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_width">@dimen/menu_icon_outside</item>
        <item name="android:layout_height">@dimen/menu_icon_outside</item>
        <item name="android:layout_marginLeft">0dp</item>
    </style>
    <style name="gravity_end">
        <item name="android:gravity">left</item>
        <item name="android:layout_gravity">left</item>
        <item name="android:layout_alignParentLeft">true</item>
    </style>

    <style name="SwitchButtonStyle">
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingRight">10dp</item>
        <item name="android:paddingTop">4dp</item>
        <item name="android:paddingBottom">4dp</item>
    </style>

    <style name="switchStyle">
        <item name="colorButtonNormal">@color/grey_background</item>
        <item name="android:colorForeground">@color/colorPrimary</item>
    </style>

    <style name="login_edittext" parent="gravity_start">
        <item name="android:singleLine">true</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColorHint">@color/colorPrimary</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@android:color/black</item>
    </style>
    <style name="login_edittext_english" parent="gravity_end">
        <item name="android:singleLine">true</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColorHint">@color/colorPrimary</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@android:color/black</item>
    </style>
    <style name="button">
        <item name="android:gravity">center</item>
        <item name="android:textSize">22sp</item>
        <item name="background">@drawable/button</item>
        <item name="android:textColor">@android:color/white</item>
    </style>

    <style name="button_orange">
        <item name="android:gravity">center</item>
        <item name="android:textSize">20sp</item>
        <item name="background">@drawable/button_orange_color</item>
        <item name="android:textColor">@android:color/white</item>
    </style>

    <style name="drawer_item_layout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">48dp</item>
        <item name="background">?attr/selectableItemBackground</item>
    </style>

    <style name="profile_lable_layout">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/text_color_black</item>
    </style>

    <style name="profile_lable_layout_value">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/grey_text</item>
    </style>

    <style name="change_pass_lable_layout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">@color/colorPrimary</item>
    </style>

    <style name="change_pass_lable_layout_description">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/text_color_black</item>
    </style>


    <style name="cancel_job_lable_texts">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/grey_text</item>
    </style>

    <style name="profile_value_layout">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:textSize">15sp</item>
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:layout_marginLeft">10dp</item>

        <item name="android:textColor">@color/text_color_black</item>
    </style>


    <style name="drawer_item_icon">
        <!--<item name="android:background">@color/colorPrimary</item>-->
        <item name="android:layout_width">24dp</item>
        <item name="android:layout_height">24dp</item>
        <item name="android:layout_gravity">right|center_vertical</item>
        <item name="android:layout_marginRight">16dp</item>
        <item name="android:scaleType">fitCenter</item>
        <item name="android:adjustViewBounds">true</item>
    </style>

    <style name="drawer_item_text">
        <item name="android:textColor">@color/grey_light</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">right|center_vertical</item>
        <item name="android:layout_marginRight">72dp</item>
    </style>

    <style name="drawer_line">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="background">@color/grey_line</item>
        <item name="android:layout_marginRight">72dp</item>
    </style>

    <style name="drawer_line_match">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="background">@color/grey_line</item>
    </style>

    <style name="drawer_line_match_color_primary">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="background">@color/colorPrimary</item>
    </style>

    <style name="switch_custom">
        <item name="android:thumb">@drawable/customswitchselector</item>
        <item name="track">@drawable/customswitchtrackselector_2</item>
        <item name="showText">true</item>
    </style>

    <style name="popupStyle">
        <item name="android:minWidth">240dp</item>
    </style>

    <style name="width">
        <item name="android:layout_width">match_parent</item>
    </style>
    <style name="edit_text">
        <item name="android:inputType">text</item>
        <item name="android:imeOptions">actionNext</item>
        <item name="android:lines">1</item>
        <item name="android:textColor">@android:color/black</item>
        <item name="android:textColorHint">@color/grey_text</item>
        <item name="android:gravity">right</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">16dp</item>
        <item name="android:layout_marginRight">16dp</item>
        <item name="android:layout_marginLeft">16dp</item>
    </style>
    <style name="fab">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_margin">16dp</item>
        <item name="android:layout_gravity">left|bottom</item>
        <item name="fabSize">normal</item>
        <item name="pressedTranslationZ">12dp</item>
        <item name="elevation">6dp</item>
        <item name="rippleColor">@android:color/white</item>
    </style>

</resources>